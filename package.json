{
  "name": "egydrip-root",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start --prefix client\" \"node server/index.js\"",
    "dev": "concurrently \"npm run dev --prefix client\" \"nodemon server/index.js\"",
